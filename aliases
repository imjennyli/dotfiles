#useful aliases

#navigation aliases
alias u='cd ../'
alias uu='cd ../../'
alias uuu='cd ../../../'
alias uuuu='cd ../../../../'
alias uuuuu='cd ../../../../../'

#Git aliases
alias ga='git add'
alias gd='git diff'
alias diff-c='git diff --cached'
alias gs='git status'
alias gl='git log'
alias gb='git branch -a'
alias fetch='git fetch --all'

alias pop='git stash pop'
alias stash='git stash'
alias sl='git stash list'

